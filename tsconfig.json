{
  "include": ["src/**/*"],
  "exclude": [],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
